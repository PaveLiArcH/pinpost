{
  "name": "ruranobe/pinpost",
  "type": "phpbb-extension",
  "description": "An extension for phpBB 3.1.x that allows users to mark a post as the 'Pin Post', which will then be quoted in the first post with a link to it.",
  "homepage": "http://www.acsyste.com",
  "version": "1.0.0",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "kinerity",
      "email": "kinerity@yahoo.com",
      "role": "Extension Developer"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "composer/installers": "~1.0"
  },
  "extra": {
    "display-name": "Pin Post",
    "soft-require": {
      "phpbb/phpbb": ">=3.1.7"
    }
  }
}
